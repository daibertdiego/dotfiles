# Applications launch
cmd + alt + ctrl - b : open -a "Brave Browser"
cmd + alt + ctrl + shift - b  : open -na "Brave Browser" --args --incognito

cmd + alt + ctrl - s : open -a "Safari"
cmd + alt + ctrl + shift - s : open -na "Safari" --args -Private

cmd + alt + ctrl - w : open -a "WhatsApp"
cmd + alt + ctrl - m : open -a "Spark"
cmd + alt + ctrl - c : open -a "Microsoft Teams (work or school)"
cmd + alt + ctrl - t : open -a "WezTerm"
cmd + alt + ctrl - i : open -a "IntelliJ IDEA CE"
cmd + alt + ctrl - f : open -a "Finder"


